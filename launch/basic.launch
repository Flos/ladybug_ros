<launch>
        <group ns="ladybug5">
                <node name="receiver"             pkg="ladybug" type="receiver"/>
                <node name="sensors"			  pkg="ladybug" type="sensor_node"/>
                <node name="image_node"           pkg="ladybug" type="image_node"/>
                <node name="service"	  		  pkg="ladybug" type="service" output="screen">
                	<param name="connection" 	value="tcp://192.168.1.10:28883"/>
            	/>
                </node>
        </group>
        
</launch>
