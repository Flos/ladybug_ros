<launch>
	<arg name="zoom"			default="2.2" />
	<arg name="nodelet_manager" default="nodelet_manager" />
	
	<node pkg="nodelet" type="nodelet" name="Rectifier_nodelet_cam0" args="load ladybug/Rectifier_nodelet $(arg nodelet_manager)">
	  	<param name="sub" 	value="/ladybug5/camera0/image_raw" />
	  	<param name="pub" 	value="/ladybug5/camera0/raw_rect" />
	  	<param name="sub_info" 	value="/ladybug5/camera0/camera_info" />
	  	<param name="zoom" 	value="$(arg zoom)" />
	  	<param name="lut" 	value="$(find ladybug)/calibration/13122828_camera0_h1024_w1224_map_" />
	</node>

	<node pkg="nodelet" type="nodelet" name="Rectifier_nodelet_cam1" args="load ladybug/Rectifier_nodelet $(arg nodelet_manager)">
